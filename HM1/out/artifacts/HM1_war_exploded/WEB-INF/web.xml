<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Part2Servlet</servlet-name>
        <servlet-class>Part2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part3Servlet</servlet-name>
        <servlet-class>Part3Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part4Servlet</servlet-name>
        <servlet-class>Part4Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part5Servlet</servlet-name>
        <servlet-class>Part5Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part6Servlet</servlet-name>
        <servlet-class>Part6Servlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>Part2Servlet</servlet-name>
        <url-pattern>/Part2Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Part3Servlet</servlet-name>
        <url-pattern>/Part3Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Part4Servlet</servlet-name>
        <url-pattern>/Part4Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Part5Servlet</servlet-name>
        <url-pattern>/Part5Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Part6Servlet</servlet-name>
        <url-pattern>/Part6Servlet</url-pattern>
    </servlet-mapping>
</web-app>